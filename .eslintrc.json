{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "no-unused-vars": 1,
        "react/prop-types": 1,
        "no-console": 0
    },
    "env": {
        "node": true
    }
}